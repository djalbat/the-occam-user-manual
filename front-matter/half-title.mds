padding: 0;
padding-top: 96pt;
  
image {
  width: 100%;
  height: auto;
  border: 2pt solid #0a0d0b;
  display: block;
  padding: 18pt 0 18pt 0;
  border-left: none;
  border-right: none;
}

primaryHeading,
secondaryHeading {
  text-align: center;
  font-weight: normal;
  margin-left: auto;
  margin-right: auto;
  text-transform: uppercase;
}

primaryHeading {
  width: 240pt;
  border: 3pt solid #0a0d0b;
  font-size: 30pt;
  border-left: none;
  border-right: none;
}

secondaryHeading {
  font-size: 18pt;

  :last-child {
    margin-top: 32pt;
  }
}
